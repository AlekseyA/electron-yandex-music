{
  "name": "electron-yandex-music",
  "productName": "Electron Yandex Music",
  "version": "1.0.0",
  "description": "Yandex music unofficial desktop app",
  "main": "src/app.js",
  "scripts": {
    "start": "electron-forge start"
  },
  "keywords": [
    "Electron",
    "yandex music",
    "demo"
  ],
  "author": "Alexey A.",
  "license": "MIT",
  "devDependencies": {
    "electron": "^13.6.6",
    "electron-prebuilt-compile": "3.0.6"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "icon": "./icon",
        "ignore": [
          "\\.idea"
        ]
      },
      "electronWinstallerConfig": {
        "name": "yandex_music"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "m8rge",
        "name": "yandex.music"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "yandexmusic"
      }
    }
  },
  "dependencies": {
    "electron-compile": "^6.4.4",
    "electron-forge": "^5.2.4",
    "electron-window-state": "^5.0.3"
  }
}